<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.myfuncdemo">

	<uses-permission android:name="android.permission.INTERNET"/>
	<application
		android:name=".application.MyApplication"
		android:allowBackup="true"
		android:icon="@mipmap/ic_launcher"
		android:label="@string/app_name"
		android:supportsRtl="true"
		android:theme="@style/StartAppTheme">

		<!--DEBUG 版本号（展示时间）-->
		<meta-data
			android:name="DEBUG_VERSION"
			android:value="${DEBUG_VERSION_VALUE}"/>


		<activity
			android:name=".SplashActivity"
			android:theme="@style/AppTheme"
			>
			<intent-filter>
				<action android:name="android.intent.action.MAIN"/>
				<category android:name="android.intent.category.LAUNCHER"/>
			</intent-filter>

		</activity>
		<activity
			android:name=".view.activity.HomeActivity"
			android:theme="@style/AppTheme">

		</activity>
		<activity
			android:name=".view.activity.NetCompareActivity"
			android:theme="@style/AppTheme">

		</activity>
		<activity
			android:name=".view.activity.ImageCompareActivity"
			android:theme="@style/AppTheme">
		</activity>
		<activity
			android:name=".view.activity.CollapsingActivity"
			android:theme="@style/AppTheme">

		</activity>
		<activity
			android:name=".MainActivity"
			android:theme="@style/AppTheme">

		</activity>
	</application>

</manifest>